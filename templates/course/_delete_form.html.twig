<form method="post" action="{{ path('app_course_delete', {'id': course.id}) }}" onsubmit="return confirm('Вы уверены что хотите удалить данный курс?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ course.id) }}">
    <button id="course-delete" class="btn btn-outline-danger">Удалить</button>
</form>